      "ðŸ”´ Attention needed:\n",
      " â€¢ Coverage error exceeds Â±5pp for percentile 0.70 (up 1h).\n",
      " â€¢ Scaled price MAE above 2% for percentile 0.05 (down 24h).\n",
      " â€¢ Directional exceed rate off by more than 5pp for percentile 0.70 (up 1h).\n"
     ]
    }
   ],
   "source": [
    "def aggregate_logret_scores(predictions: pd.DataFrame, metrics: pd.DataFrame) -> pd.DataFrame:\n",
    "    group_keys = [\"percentile\", \"direction\", \"horizon_hours\"]\n",
    "\n",
    "    def _directional_exceed_rate(group: pd.DataFrame) -> float:\n",
    "        direction = group.name[1] if isinstance(group.name, tuple) else None\n",
    "        if direction == \"up\":\n",
    "            return (group[\"actual\"] > group[\"prediction\"]).mean()\n",
    "        if direction == \"down\":\n",
    "            return (group[\"actual\"] < group[\"prediction\"]).mean()\n",
    "        return np.nan\n",
    "\n",
    "    coverage_df = (\n",
    "        predictions.groupby(group_keys)\n",
    "        .apply(\n",
    "            lambda g: pd.Series({\n",
    "                \"empirical_coverage\": (g[\"actual\"] <= g[\"prediction\"]).mean(),\n",
    "                \"mean_signed_error\": (g[\"prediction\"] - g[\"actual\"]).mean(),\n",
    "                \"directional_exceed_pct\": _directional_exceed_rate(g),\n",
    "            }),\n",
    "            include_groups=False,\n",
    "        )\n",
    "        .reset_index()\n",
    "    )\n",
    "\n",
    "    metric_df = (\n",
    "        metrics.groupby(group_keys, as_index=False)\n",
    "        .agg(\n",
    "            avg_pinball=(\"pinball_loss\", \"mean\"),\n",
    "            avg_price_mae=(\"price_mae\", \"mean\"),\n",
    "            avg_prev_close=(\"avg_prev_close\", \"mean\"),\n",
    "        )\n",
    "    )\n",
    "    metric_df[\"scaled_price_mae_pct\"] = metric_df[\"avg_price_mae\"] / metric_df[\"avg_prev_close\"]\n",
    "\n",
    "    summary_table = metric_df.merge(coverage_df, on=group_keys, how=\"left\")\n",
    "    summary_table[\"coverage_error\"] = summary_table[\"empirical_coverage\"] - summary_table[\"percentile\"]\n",
    "    summary_table = summary_table.rename(columns={\"avg_prev_close\": \"avg_prev_close_price\"})\n",
    "    summary_table[\"avg_price_mae\"] = summary_table[\"avg_price_mae\"].round().astype(\"Int64\")\n",
    "    summary_table[\"expected_exceed_pct\"] = np.where(\n",
    "        summary_table[\"direction\"] == \"up\",\n",
    "        1 - summary_table[\"percentile\"],\n",
    "        np.where(summary_table[\"direction\"] == \"down\", summary_table[\"percentile\"], np.nan),\n",
    "    )\n",
    "    summary_table[\"exceed_error\"] = summary_table[\"directional_exceed_pct\"] - summary_table[\"expected_exceed_pct\"]\n",
    "\n",
    "    column_order = [\n",
    "        \"percentile\",\n",
    "        \"direction\",\n",
    "        \"horizon_hours\",\n",
    "        \"avg_pinball\",\n",
    "        \"scaled_price_mae_pct\",\n",
    "        \"empirical_coverage\",\n",
    "        \"coverage_error\",\n",
    "        \"directional_exceed_pct\",\n",
    "        \"expected_exceed_pct\",\n",
    "        \"exceed_error\",\n",
    "        \"avg_price_mae\",\n",
    "        \"avg_prev_close_price\",\n",
    "        \"mean_signed_error\",\n",
    "    ]\n",
    "    ordered = summary_table[column_order].sort_values([\"percentile\", \"direction\", \"horizon_hours\"])\n",
    "    return ordered\n",
    "\n",
    "\n",
    "logret_summary = aggregate_logret_scores(predictions_df, metrics_df)\n",
    "overall_pinball = metrics_df[\"pinball_loss\"].mean()\n",
    "overall_scaled_price_mae = metrics_df[\"price_mae\"].mean() / metrics_df[\"avg_prev_close\"].mean()\n",
    "\n",
    "print(f\"Overall mean pinball loss: {overall_pinball:.6f}\")\n",
    "print(f\"Overall scaled price MAE: {overall_scaled_price_mae:.4%}\")\n",
    "display(logret_summary)\n",
    "\n",
    "alerts: list[str] = []\n",
    "if logret_summary[\"coverage_error\"].abs().max() > 0.05:\n",
    "    worst_row = logret_summary.loc[logret_summary[\"coverage_error\"].abs().idxmax()]\n",
    "    alerts.append(\n",
    "        \"Coverage error exceeds Â±5pp for percentile \"\n",
    "        f\"{worst_row['percentile']:.2f} ({worst_row['direction']} {int(worst_row['horizon_hours'])}h).\"\n",
    "    )\n",
    "\n",
    "if logret_summary[\"scaled_price_mae_pct\"].max() > 0.02:\n",
    "    worst_row = logret_summary.loc[logret_summary[\"scaled_price_mae_pct\"].idxmax()]\n",
    "    alerts.append(\n",
    "        \"Scaled price MAE above 2% for percentile \"\n",
    "        f\"{worst_row['percentile']:.2f} ({worst_row['direction']} {int(worst_row['horizon_hours'])}h).\"\n",
    "    )\n",
    "\n",
    "if logret_summary[\"exceed_error\"].abs().max() > 0.05:\n",
    "    worst_row = logret_summary.loc[logret_summary[\"exceed_error\"].abs().idxmax()]\n",
    "    alerts.append(\n",
    "        \"Directional exceed rate off by more than 5pp for percentile \"\n",
    "        f\"{worst_row['percentile']:.2f} ({worst_row['direction']} {int(worst_row['horizon_hours'])}h).\"\n",
    "    )\n",
    "\n",
    "if alerts:\n",
    "    print(\"ðŸ”´ Attention needed:\")\n",
    "    for note in alerts:\n",
    "        print(f\" â€¢ {note}\")\n",
    "else:\n",
    "    print(\"âœ… No major calibration or price-error flags (coverage, exceedance, MAE all within thresholds).\")"
   ]
