    "METRICS_PATH = OUTPUT_DIR / \"final_model_testing_metrics.parquet\"\n",
    "\n",
    "if SAVE_RESULTS:\n",
    "    predictions_df.to_parquet(PREDICTIONS_PATH)\n",
    "    metrics_df.to_parquet(METRICS_PATH)\n",
    "    print(f\"Saved predictions to {PREDICTIONS_PATH}\")\n",
    "    print(f\"Saved metrics to {METRICS_PATH}\")\n",
    "else:\n",
    "    print(\"Skipping persistence (set SAVE_RESULTS=True to enable).\")"
   ]
  },
