   ],
   "source": [
    "bundles = discover_bundles(MODELS_DIR)\n",
    "predictions_df = generate_prediction_records(bundles, X_test, y_test, window_prev_close)\n",
    "print(f\"Prediction rows generated: {len(predictions_df):,}\")\n",
    "if not predictions_df.empty:\n",
    "    display(predictions_df)\n",
    "else:\n",
    "    print(\"No predictions available. Check earlier warnings for details.\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "1cb9e869",
   "metadata": {},
   "outputs": [
    {
     "ename": "NameError",
     "evalue": "name 'model_direction_summary' is not defined",
     "output_type": "error",
     "traceback": [
      "\u001b[31m---------------------------------------------------------------------------\u001b[39m",
      "\u001b[31mNameError\u001b[39m                                 Traceback (most recent call last)",
