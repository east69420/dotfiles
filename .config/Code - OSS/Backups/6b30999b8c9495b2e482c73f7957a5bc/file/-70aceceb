   "source": [
    "group_cols = [\"target\", \"percentile\", \"stage\", \"horizon_hours\", \"direction\"]\n",
    "EXCEED_TOLERANCE = 0.05\n",
    "\n",
    "def summarize_over_under(df: pd.DataFrame) -> pd.DataFrame:\n",
    "    if df.empty:\n",
    "        return pd.DataFrame()\n",
    "    working = df.copy()\n",
    "    working[\"error\"] = working[\"prediction\"] - working[\"actual\"]\n",
    "    working[\"price_error\"] = working[\"pred_price\"] - working[\"actual_price\"]\n",
    "    working[\"directional_exceed\"] = np.where(\n",
    "    working[\"direction\"] == \"up\",\n",
    "        working[\"actual_price\"] > working[\"pred_price\"],\n",
    "        np.where(\n",
    "            working[\"direction\"] == \"down\",\n",
    "            working[\"actual_price\"] < working[\"pred_price\"],\n",
    "            False,\n",
    "        ),\n",
    "    )\n",
    "    working[\"exceed_return_gap\"] = np.where(\n",
    "        working[\"directional_exceed\"],\n",
    "        working[\"actual\"] - working[\"prediction\"],\n",
    "        np.nan,\n",
    "    )\n",
    "    working[\"exceed_price_gap\"] = np.where(\n",
    "        working[\"directional_exceed\"],\n",
    "        working[\"actual_price\"] - working[\"pred_price\"],\n",
    "        np.nan,\n",
    "    )\n",
    "    summary = (\n",
    "        working.groupby(group_cols)\n",
    "        .agg(\n",
    "            count=(\"error\", \"size\"),\n",
    "            over_count=(\"error\", lambda x: (x > 0).sum()),\n",
    "            under_count=(\"error\", lambda x: (x < 0).sum()),\n",
    "            mean_error=(\"error\", \"mean\"),\n",
    "            median_error=(\"error\", \"median\"),\n",
    "            mean_abs_error=(\"error\", lambda x: np.mean(np.abs(x))),\n",
    "            mean_price_error=(\"price_error\", \"mean\"),\n",
    "            median_price_error=(\"price_error\", \"median\"),\n",
    "            mean_abs_price_error=(\"price_error\", lambda x: np.mean(np.abs(x))),\n",
    "            exceed_count=(\"directional_exceed\", \"sum\"),\n",
    "            exceed_pct=(\"directional_exceed\", \"mean\"),\n",
    "            mean_exceed_return_gap=(\"exceed_return_gap\", \"mean\"),\n",
    "            median_exceed_return_gap=(\"exceed_return_gap\", \"median\"),\n",
    "            mean_exceed_price_gap=(\"exceed_price_gap\", \"mean\"),\n",
    "            median_exceed_price_gap=(\"exceed_price_gap\", \"median\"),\n",
    "            mean_abs_exceed_price_gap=(\"exceed_price_gap\", lambda x: x.dropna().abs().mean()),\n",
    "        )\n",
    "        .reset_index()\n",
    "    )\n",
    "    summary[\"over_pct\"] = summary[\"over_count\"] / summary[\"count\"]\n",
    "    summary[\"under_pct\"] = summary[\"under_count\"] / summary[\"count\"]\n",
    "    summary[\"expected_exceed_pct\"] = np.where(\n",
    "        summary[\"direction\"] == \"up\",\n",
    "        1 - summary[\"percentile\"],\n",
    "        np.where(summary[\"direction\"] == \"down\", summary[\"percentile\"], np.nan),\n",
    "    )\n",
    "    summary[\"exceed_error\"] = summary[\"exceed_pct\"] - summary[\"expected_exceed_pct\"]\n",
    "    summary[\"mean_exceed_price_gap_abs\"] = summary[\"mean_abs_exceed_price_gap\"]\n",
    "    summary[\"mean_exceed_return_gap_abs\"] = summary[\"mean_exceed_return_gap\"].abs()\n",
    "    return summary.sort_values([\"target\", \"percentile\"])\n",
    "\n",
    "model_direction_summary = summarize_over_under(predictions_df)\n",
    "print(f\"Models summarized: {model_direction_summary['target'].nunique()} targets\")\n",
    "focus_columns = [\n",
    "    \"target\",\n",
    "    \"percentile\",\n",
    "    \"direction\",\n",
    "    \"horizon_hours\",\n",
    "    \"count\",\n",
    "    \"exceed_pct\",\n",
    "    \"expected_exceed_pct\",\n",
    "    \"exceed_error\",\n",
    "    \"mean_exceed_price_gap\",\n",
    "    \"mean_exceed_price_gap_abs\",\n",
    "    \"mean_exceed_return_gap\",\n",
    "    \"mean_exceed_return_gap_abs\",\n",
    "    \"mean_abs_price_error\",\n",
    "    \"over_pct\",\n",
    "    \"under_pct\",\n",
    " ]\n",
    "display(\n",
    "    model_direction_summary[focus_columns]\n",
    "    .sort_values(\"exceed_error\", ascending=False)\n",
    "    .head(10)\n",
    ")\n",
    "exceed_flags = model_direction_summary[\n",
    "    (model_direction_summary[\"expected_exceed_pct\"].notna())\n",
    "    & (model_direction_summary[\"exceed_error\"] > EXCEED_TOLERANCE)\n",
    "].sort_values(\"exceed_error\", ascending=False)\n",
    "if not exceed_flags.empty:\n",
    "    print(\n",
    "        f\"ðŸ”´ Underprediction hot spots (actuals beyond quantile > expected + {EXCEED_TOLERANCE:.0%}):\",\n",
    "    )\n",
    "    display(\n",
    "        exceed_flags[\n",
    "            [\n",
    "                \"target\",\n",
    "                \"percentile\",\n",
    "                \"direction\",\n",
    "                \"horizon_hours\",\n",
    "                \"exceed_pct\",\n",
    "                \"expected_exceed_pct\",\n",
    "                \"exceed_error\",\n",
    "                \"mean_exceed_return_gap\",\n",
    "                \"mean_exceed_price_gap\",\n",
    "                \"mean_exceed_price_gap_abs\",\n",
    "            ]\n",
    "        ]\n",
    "    )\n",
    "else:\n",
    "    print(f\"âœ… All models stay within the {EXCEED_TOLERANCE:.0%} exceed tolerance.\")"
   ]
  },
