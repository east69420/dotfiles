    "    }\n",
    "\n",
    "if PATH_OVERRIDES:\n",
    "    resolved_overrides |= {k: Path(v).expanduser().resolve() for k, v in PATH_OVERRIDES.items()}\n",
    "\n",
    "def _resolve_path(key: str) -> Path:\n",
    "    target = resolved_overrides.get(key, DEFAULT_PATHS[key])\n",
    "    target = Path(target).expanduser()\n",
    "    return target.resolve()\n",
    "\n",
    "DATA_ROOT = _resolve_path(\"root\")\n",
    "CACHE_PATH = _resolve_path(\"feature_cache\")\n",
    "MODELS_DIR = _resolve_path(\"trained_models\")\n",
